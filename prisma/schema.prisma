generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model departement {
  id         Int        @id @default(autoincrement())
  code_aff   String?    @db.VarChar
  nom_depart String?    @db.VarChar
  fonction   fonction[]
}

model fonction {
  lib_fonction  String?
  code_fonction Int           @id @unique @default(autoincrement())
  dep           Int?
  Utilisateur   Utilisateur?
  droit_accee   droit_accee[]
  departement   departement?  @relation(fields: [dep], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model Utilisateur {
  flgstatut       String?             @db.VarChar
  affectation     Int?
  tel_chargee     String?             @db.VarChar
  affectation_c   String?             @db.VarChar
  affectation_d   String?             @db.VarChar
  code_fonction   Int                 @unique @default(autoincrement())
  email_chargee   String?             @db.VarChar
  usr_matricule   String?             @db.VarChar
  usr_nomprenom   String?             @db.VarChar
  usr_motdepasse  String?             @db.VarChar
  affectation_sog String?             @db.VarChar
  id              Int                 @id @default(autoincrement())
  created_at      DateTime?           @default(now()) @db.Timestamp(6)
  updated_at      DateTime?           @default(now()) @db.Timestamp(6)
  comptes         Compte[]
  fonction        fonction            @relation(fields: [code_fonction], references: [code_fonction], onDelete: NoAction, onUpdate: NoAction)
  client_recouv   v_client_recouv?
  obsClients      ImpObstransclient[] @relation("ObsClients")
}

model Region {
  codug   Int     @id @unique(map: "Region_id_key") @default(autoincrement())
  libelle String? @db.VarChar
  Zone    Zone[]
}

model Zone {
  libelle   String?     @db.VarChar
  codugr    Int
  codug     Int         @id @unique @default(autoincrement())
  Agence    Agence[]
  Compte    Compte[]
  Region    Region      @relation(fields: [codugr], references: [codug])
  ab_client ab_client[]
  ab_compte ab_compte[]
}

model Agence {
  codug     Int         @id @default(autoincrement())
  libelle   String?     @db.VarChar
  codugz    Int
  Zone      Zone        @relation(fields: [codugz], references: [codug], onDelete: NoAction, onUpdate: NoAction)
  Compte    Compte[]
  ab_client ab_client[]
  ab_compte ab_compte[]
}

