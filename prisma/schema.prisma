model ImpObstransclient {
  id           Int           @id @unique(map: "uniqueImp_id") @default(autoincrement())
  obs          String?       @db.VarChar(4000)
  numobs       Int?
  cli          String?       @db.VarChar(20)
  dateobs      DateTime?     @db.Date
  etat_prop    String?       @db.VarChar(1)
  transId      Int?
  mottId       Int?
  vtrfId       Int?
  mott         Mott?         @relation(fields: [mottId], references: [id])
  trans        Trans?        @relation(fields: [transId], references: [id])
  vtrf         VTRF?         @relation(fields: [vtrfId], references: [id])
  utilisateurs Utilisateur[] @relation("ObsClients")
}

model Mott {
  id      Int                 @id @unique(map: "uniqueMott_id") @default(autoincrement())
  libelle String?             @db.VarChar(100)
  condenv String?             @unique @db.VarChar
  impobs  ImpObstransclient[]
}

model Trans {
  id      Int                 @id @unique @default(autoincrement())
  libelle String?             @db.VarChar(100)
  codenv  String              @unique @db.VarChar
  impobs  ImpObstransclient[]
}

model VTRF {
  id       Int                 @id @unique(map: "uniquevtrf_id") @default(autoincrement())
